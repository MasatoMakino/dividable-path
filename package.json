{
  "name": "particle-waypoint",
  "version": "0.4.0",
  "description": "Path module for calculating waypoints.",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "module": "./esm/index.js",
  "files": [
    "lib",
    "esm"
  ],
  "repository": "https://MasatoMakino@github.com/MasatoMakino/particle-waypoint.git",
  "author": "MasatoMakino <unagiinu.g@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^24.0.25",
    "husky": "^4.0.1",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "replace-in-file": "^5.0.2",
    "ts-jest": "^24.3.0",
    "typedoc": "^0.15.6",
    "typescript": "^3.7.4"
  },
  "scripts": {
    "test": "jest --coverage",
    "build": "tsc -p tsconfig.json && tsc -p tsconfig.esm.json && npm run doc",
    "watch": "npm run watch:tsc",
    "watch:tsc": "tsc -w",
    "doc": "npm run doc:generate && npm run doc:replaceNode && npm run doc:card",
    "doc:generate": "typedoc --tsconfig tsdocconfig.json src",
    "doc:replaceNode": "node ./task/replaceAPI.js",
    "doc:card": "repoimg -n MasatoMakino/particle-waypoint -o ./docs/card.png"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {
    "raf-ticker": "https://github.com/MasatoMakino/raf-ticker.git"
  }
}
